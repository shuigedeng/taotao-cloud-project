        "type":"keyword"
      },
      "trace_id": {
        "type":"keyword"
      },
      "level": {
        "type":"long",
        "null_value": 3
      },
      "tag": {
        "type":"keyword"
      },
      "tag1": {
        "type":"keyword"
      },
      "tag2": {
        "type":"keyword"
      },
      "tag3": {
        "type":"keyword"
      },
      "content": {
        "type": "text"
      },
