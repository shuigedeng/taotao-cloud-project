{
  "name": "taotao-cloud-mall-react",
  "version": "1.0.0",
  "private": true,
  "description": "滔滔商城小程序",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "taotao-cloud-mall:build:weapp": "taro build --type weapp",
    "taotao-cloud-mall:build:swan": "taro build --type swan",
    "taotao-cloud-mall:build:h5": "taro build --type h5",
    "taotao-cloud-mall:build:alipay": "taro build --type alipay",
    "taotao-cloud-mall:build:tt": "taro build --type tt",
    "taotao-cloud-mall:build:rn": "taro build --type rn",
    "taotao-cloud-mall:build:qq": "taro build --type qq",
    "taotao-cloud-mall:build:jd": "taro build --type jd",
    "taotao-cloud-mall:build:quickapp": "taro build --type quickapp",
    "taotao-cloud-mall:dev:weapp": "npm run build:weapp -- --watch",
    "taotao-cloud-mall:dev:swan": "npm run build:swan -- --watch",
    "taotao-cloud-mall:dev:alipay": "npm run build:alipay -- --watch",
    "taotao-cloud-mall:dev:tt": "npm run build:tt -- --watch",
    "taotao-cloud-mall:dev:h5": "cross-env NODE_ENV=mock npm run build:h5 -- --watch",
    "taotao-cloud-mall:dev:rn": "npm run build:rn -- --watch",
    "taotao-cloud-mall:dev:qq": "npm run build:qq -- --watch",
    "taotao-cloud-mall:dev:jd": "npm run build:jd -- --watch",
    "taotao-cloud-mall:dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.0.18",
    "@tarojs/components": "3.0.18",
    "@tarojs/plugin-babel": "^2.2.10",
    "@tarojs/plugin-mock": "^0.0.6",
    "@tarojs/react": "3.0.18",
    "@tarojs/runtime": "3.0.18",
    "@tarojs/taro": "3.0.18",
    "cross-env": "^7.0.3",
    "lodash": "4.17.15",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sa-sdk-javascript": "^1.16.3",
    "sa-sdk-miniprogram": "^1.14.5",
    "taro-plugin-tailwind": "^1.0.6",
    "taro-ui": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tarojs/mini-runner": "3.0.18",
    "@tarojs/webpack-runner": "3.0.18",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "@typescript-eslint/parser": "^2.x",
    "babel-preset-taro": "3.0.18",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.0.18",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "stylelint": "9.3.0",
    "typescript": "^3.7.0"
  }
}
